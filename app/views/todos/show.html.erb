<div class="card-deck">
    <div class="card">
       <%= image_tag @todos.picture , class:'card-omg-top', 'height': '500px', 'width': '200px'%>
       <div class="card-body">
            <p class="card-text"><%= @todos.description %></p>
            <p class="card-text"<%= @todos.completed ?  'True' : 'False' %>></p>
      </div>
    </div>
</div>
<%= link_to 'Back to main', root_path %>